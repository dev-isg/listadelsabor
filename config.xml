<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.platos.free.player.listadelsabor"
        version   = "1.0.0">

   <name>Lista del Sabor</name>

   <description>listadelsabor es una aplicaci√≥n basica dodne usted podra encontrar una el plato que mas le guste saborear hoy.</description>

   <author href="http://innovationssystems.com/" email="innovationssystems@gmail.com">Innovations Systems Group</author>

   <feature name="http://api.phonegap.com/1.0/network"/>
   <feature name="http://api.phonegap.com/1.0/file"/>
   <feature name="http://api.phonegap.com/1.0/notification"/>

   <preference name="phonegap-version" value="2.9.0" />
   <preference name="target-device" value="universal" />

   <access origin=".*" />

   <!-- Icons -->
   <icon src="icon.png" width="64" height="64" gap:role="default" />
   <icon src="img/icon-72x72.png" width="72" height="72" gap:platform="android" gap:density="hdpi" />
   <icon src="img/icon-96x96.png" width="96" height="96" gap:platform="android" gap:density="xhdpi" />
   <icon src="img/icon-72x72.png" width="72" height="72" gap:platform="ios" />

   <!-- Splash Screens -->
   <gap:splash src="splash.png" />
   <gap:splash src="img/splash-160x220.png" gap:platform="android" gap:density="ldpi" />
   <gap:splash src="splash.png" gap:platform="android" gap:density="mdpi" />
   <gap:splash src="img/splash-450x650.png" gap:platform="android" gap:density="hdpi" />
</widget>